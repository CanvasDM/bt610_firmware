{
    "configurations": [
        {
            "name": "zephyr",
            "browse": {
              "path": ["${workspaceFolder}"],
              "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
              "${workspaceFolder}/../zephyr/**",
              "${workspaceFolder}/../modules/**",
              "${workspaceFolder}/../nrf/**",
              "${workspaceFolder}/../nrfxlib/**",
              "${workspaceFolder}/../mbedtls/**",
              "${workspaceFolder}/common/**"
            ],
            "cStandard": "c99",
            "cppStandard": "c++98",
            "intelliSenseMode": "gcc-arm"
          }
    ],
    "version": 4
}